                    xmlns:local="clr-namespace:Calculator.Resources"
                    xmlns:viewmodel="clr-namespace:Calculator.ViewModel"
                    xmlns:convert="clr-namespace:Calculator.ViewModel.ConvertViewModel">
    <viewmodel:MainViewModel x:Key="mainViewModel"></viewmodel:MainViewModel>
    <viewmodel:StandardViewModel x:Key="standardViewModel"></viewmodel:StandardViewModel>
    <viewmodel:SettingViewModel x:Key="settingViewModel"></viewmodel:SettingViewModel>
    <viewmodel:DateCalculationViewModel x:Key="dateViewModel"></viewmodel:DateCalculationViewModel>
